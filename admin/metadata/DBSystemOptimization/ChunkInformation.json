{
  "layout": {
    "container": "panel",
    "metadata": {
      "items": [
        {
          "style": {
            "flex": 1
          },
          "equally": true,
          "columns": [
            {
              "slot": "grid",
              "style": {
                "flex": 24
              }
            }
          ]
        }
      ]
    }
  },
  "components": {
    "grid": {
      "id": "grid",
      "component": "grid",
      "metadata": {
        "pagination": false,
        "columns": {
          "method": "remote",
          "url": "/GridMetadata/DBSystemOptimization/ChunkInformation"
        },
        "source": {
          "method": "static",
          "data": []
        },
        "events": {}
      }
    }
  },
  "http": [
    {
      "method": "get",
      "url": "/GridData/DBSystemOptimization/ChunkInformation",
      "time": 3000,
      "autostart": true,
      "repeat": true,
      "immediate": true,
      "success":{
        "trigger": [
          {
            "target": "grid",
            "method": "setData",
            "params": ["response.data"]
          }
        ]
      },
      "error":{
        "message": "请求错误!"
      }
    }
  ]
}
