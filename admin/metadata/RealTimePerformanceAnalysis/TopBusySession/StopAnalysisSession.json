{
  "view": {
    "name": "dropdown-view"
  },
  "layout": {
    "container": "panel",
    "metadata": {
      "items": [
        {
          "style": {
            "flex": 1
          },
          "equally": true,
          "columns": [
            {
              "slot": "toolbar",
              "style": {
                "flex": 24
              }
            }
          ]
        },
        {
          "style": {
            "flex": 10
          },
          "equally": true,
          "columns": [
            {
              "slot": "grid",
              "style": {
                "flex": 24
              }
            }
          ]
        },
        {
          "style": {
            "flex": 6
          },
          "equally": true,
          "columns": [
            {
              "slot": "chart",
              "style": {
                "flex": 24
              }
            }
          ]
        },
        {
          "style": {
            "flex": 1
          },
          "equally": true,
          "columns": [
            {
              "slot": "toolbar2",
              "style": {
                "flex": 24
              }
            }
          ]
        }
      ]
    }
  },
  "components": {
    "toolbar": {
      "component": "toolbar",
      "metadata": {
        "children": {
          "method": "static",
          "data": [
            {
              "id": "checks",
              "widget": "checks",
              "position": "right",
              "props": {
                "label": "label",
                "value": "visible"
              }
            }
          ]
        },
        "events": {
          "click#click": [
            {
              "method": "loadData",
              "params": []
            }
          ]
        }
      },
      "id": "toolbar"
    },
    "grid": {
      "id": "grid",
      "component": "grid",
      "metadata": {
        "pagination": false,
        "columns": {
          "method": "remote",
          "url": "/GridMetadata/RealTimePerformanceAnalysis/StopAnalysisSession"
        },
        "source": {
          "method": "static",
          "data": []
        },
        "events": {
          "columns-change": [
            {
              "target": "toolbar",
              "method": "trigger",
              "params": [
                "\"checks\"",
                "\"setChecks\"",
                "columns"
              ]
            }
          ],
          "row-click": [
            {
              "target": "toolbar2",
              "method": "trigger",
              "params": [
                "\"List_Analysis_Result\"",
                "\"setQuery\"",
                "{ session: row.session }"
              ]
            }
          ]
        }
      }
    },
    "chart": {
      "id": "chart",
      "component": "vechart",
      "metadata": {
        "settings": {
          "type": "histogram",
          "metrics": [
            "buf reads"
          ],
          "dimension": [
            "id"
          ],
          "labelMap": {
            "buf reads": "Buf Reads",
            "id": "ID"
          }
        },
        "data": {
          "columns": [
            "buf reads",
            "id"
          ],
          "rows": []
        }
      }
    },
    "toolbar2": {
      "component": "toolbar",
      "metadata": {
        "children": {
          "method": "static",
          "data": [
            {
              "id": "List_Analysis_Result",
              "widget": "routerLink",
              "position": "right",
              "label": "List Analysis Result",
              "type": "primary",
              "icon": "el-icon-caret-bottom",
              "checkQueryNotNull": true,
              "checkQueryNotNullErrorMessage": "请选择数据行",
              "to": { "path": "List_Analysis_Result" }
            }
          ]
        },
        "events": {}
      },
      "id": "toolbar2"
    }
  },
  "http": [
    {
      "name": "loadData",
      "method": "get",
      "url": "/app-sonar-0.1/immediatelyCommand/stopApAnalysis",
      "params":{
        "sessionId": "this.GLOBAL.route.query.session"
      },
      "time": 1000,
      "autostart": true,
      "repeat": false,
      "immediate": false,
      "success": {
        "trigger": [
          {
            "target": "grid",
            "method": "setData",
            "params": [
              "response.data.data.items"
            ]
          }
        ]
      },
      "error": {
        "message": "请求错误!"
      }
    }
  ]
}
